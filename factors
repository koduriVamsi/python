def factor(n,o):
    i=1
    count=0
    a=[]
    while(i<=n):
        if (n%i==0):
            count+=1
            a.append(i)
        i+=1
    print("Number of factors:",count)
    print("1*",o,"factor is:",a[0:o])
a=int(input("Enter number:"))
o=int(input("Enter N:"))
factor(a,o)


output:
Enter number:1
Enter N:2
Number of factors: 1
1* 2 factor is: [1]

